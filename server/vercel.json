{
    "version": 2,
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "outputDirectory": "src",
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ]
}
